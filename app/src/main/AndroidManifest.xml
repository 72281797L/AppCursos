        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".actividades.UsuariosActivity"></activity>
        <activity android:name=".actividades.ProfesoresActivity" />
        <activity android:name=".actividades.AsignaturasActivity" />
        <activity android:name=".actividades.AlumnosActivity" />
        <activity android:name=".actividades.CursosActivity" />
        <activity android:name=".actividades.MenuActivity" />
        <activity android:name=".actividades.RegistrarseActivity" />
        <activity android:name=".actividades.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>