<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".actividades.MapaActivity">

    <fragment
        xmlns:map="http://schemas.android.com/apk/res-auto"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        map:cameraTargetLat="47.621120"
        map:cameraTargetLng="-122.349594"
        map:cameraZoom="15"
        tools:context=".actividades.MapaActivity" />

    <Button
        android:id="@+id/btn_rutas_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/boton_redondeados"
        android:textColor="@color/icons"
        android:textSize="20sp"
        android:onClick="anadirRuta"
        android:layout_margin="10dp"
        android:text="@string/rutas"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>
